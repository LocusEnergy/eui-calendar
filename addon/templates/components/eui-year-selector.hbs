<ol class="eui-year-selector--years">

  {{#each years as |year|}}
    {{#if hasBlock}}
      {{yield year}}
    {{else}}
      {{eui-interval
        moment=year
        interval='year'
        is-disabled=(not (moment-same-decade decade year))
        is-selected=(contains-year (get selection '[]') year)
      }}
    {{/if}}
  {{/each}}

</ol>
