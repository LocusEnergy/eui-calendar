<ol class="eui-year-selector--years">

  {{#each years as |year|}}
    {{#if hasBlock}}
      {{yield year}}
    {{else}}
      {{eui-interval
        moment=year
        interval='year'
        on-select=(action 'selectYear')
        is-disabled=(not (moment-same-decade decade year))
      }}
    {{/if}}
  {{/each}}

</ol>
