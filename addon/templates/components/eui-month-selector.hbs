<ol class="eui-month-selector--months">
  {{#each months as |month|}}
    {{#if hasBlock}}
      {{yield month}}
    {{else}}
      {{eui-interval
        tagName="li"
        moment=month
        interval='month'
        is-selected=(contains-month (get selection '[]') month)
      }}
    {{/if}}
  {{/each}}
</ol>
